type teacher {
  id: ID
  name: String
  bio: String
  avatar_url: String
}

type subscriber {
  id: ID
  name: String
  email: String
}

type Query {
  listTeacher: [teacher]
  findTeacher(id: ID!): teacher
}

type Mutation {
  createTeacher(name: String!, bio: String, avatar_url: String): teacher
}
